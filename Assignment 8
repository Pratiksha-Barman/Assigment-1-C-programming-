//Write a program to check whether a given number is divisible by 3 and divisible by 2.
#include <stdio.h>
int main() 
{
    int num;
    printf("Enter a number \n");
    scanf("%d",& num);
        if(num%2==0 && num%3==0)
        printf("The number is divisible by 2 and 3");
        else 
        printf("The number is not divisible by 2 and 3");
    return 0;
}

